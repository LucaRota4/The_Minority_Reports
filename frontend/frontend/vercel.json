{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  },
   "rewrites": [
        {
            "source": "/app/docs",
            "destination": "https://proxy.gitbook.site/sites/site_UP1N2"
        },
        {
            "source": "/app/docs/:match*",
            "destination": "https://proxy.gitbook.site/sites/site_UP1N2/:match*"
        }
    ]
}